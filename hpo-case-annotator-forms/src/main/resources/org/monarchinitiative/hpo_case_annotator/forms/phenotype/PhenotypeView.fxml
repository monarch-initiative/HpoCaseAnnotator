<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import org.monarchinitiative.hpo_case_annotator.forms.phenotype.*?>

<VBox xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="org.monarchinitiative.hpo_case_annotator.forms.phenotype.PhenotypeViewController">
    <stylesheets>
        <URL value="@../base.css" />
        <URL value="@phenotype-view.css" />
    </stylesheets>
   <children>
      <SplitPane dividerPositions="0.5" VBox.vgrow="ALWAYS">
        <items>
            <AnchorPane>
                <children>
                    <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                        <TableView fx:id="phenotypeTerms">
                            <columns>
                                <TableColumn fx:id="termIdColumn" text="ID" />
                            </columns>
                        </TableView>
                    </VBox>
                </children>
            </AnchorPane>
            <AnchorPane id="phenotype-feature-pane">
                <children>
                    <PhenotypicFeature fx:id="phenotypicFeature" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                </children>
            </AnchorPane>
        </items>
      </SplitPane>
   </children>
</VBox>
