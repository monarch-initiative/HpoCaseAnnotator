<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox id="phenotypicFeaturesTable" prefHeight="250.0" styleClass="section" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.monarchinitiative.hpo_case_annotator.forms.phenotype.PhenotypicFeaturesTableController">
    <children>
      <HBox prefHeight="100.0" prefWidth="200.0" styleClass="subsection">
         <children>
            <Label styleClass="title" text="Approved phenotype terms" />
         </children>
      </HBox>
        <TableView fx:id="termsTableView" editable="true" VBox.vgrow="SOMETIMES">
            <columns>
                <TableColumn fx:id="termIdTableColumn" editable="false" minWidth="80.0" resizable="false" text="Term ID" />
                <TableColumn fx:id="labelTableColumn" editable="false" minWidth="180.0" prefWidth="180.0" text="Label" />
                <TableColumn fx:id="onsetTableColumn" editable="false" minWidth="90.0" prefWidth="90.0" text="Onset" />
                <TableColumn fx:id="resolutionTableColumn" editable="false" minWidth="90.0" prefWidth="90.0" text="Resolution" />
                <TableColumn fx:id="presentTableColumn" editable="false" minWidth="75.0" prefWidth="75.0" text="Present" />
            </columns>
            <placeholder>
                <Label>No data to display</Label>
            </placeholder>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
         <VBox.margin>
            <Insets bottom="5.0" top="5.0" />
         </VBox.margin>
        </TableView>
      <HBox alignment="CENTER_RIGHT" VBox.vgrow="NEVER">
         <children>
            <Button fx:id="removeButton" disable="true" minHeight="30.0" minWidth="135.0" mnemonicParsing="false" onAction="#removeButtonAction" prefHeight="30.0" prefWidth="135.0" text="Remove selected" />
         </children>
         <padding>
            <Insets bottom="5.0" top="5.0" />
         </padding>
      </HBox>
    </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</VBox>
