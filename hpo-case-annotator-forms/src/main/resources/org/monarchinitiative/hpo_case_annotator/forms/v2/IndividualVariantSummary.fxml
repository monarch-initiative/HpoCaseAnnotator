<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.monarchinitiative.hpo_case_annotator.forms.v2.IndividualVariantSummaryController">
    <children>
        <TableView fx:id="variantTableView" editable="true" VBox.vgrow="ALWAYS">
            <columns>
                <TableColumn fx:id="genotypeTableColumn" maxWidth="-1.0" minWidth="200.0" prefWidth="-1.0" text="Genotype" />
                <TableColumn fx:id="variantIdTableColumn" editable="false" maxWidth="-1.0" minWidth="120.0" prefWidth="-1.0" text="Id" />
                <TableColumn fx:id="genomicAssemblyTableColumn" editable="false" maxWidth="-1.0" minWidth="120.0" prefWidth="-1.0" text="Assembly" />
                <TableColumn fx:id="contigTableColumn" editable="false" maxWidth="-1.0" minWidth="100.0" prefWidth="-1.0" text="Contig" />
                <TableColumn fx:id="startTableColumn" editable="false" maxWidth="-1.0" minWidth="100.0" prefWidth="-1.0" text="Start" />
                <TableColumn fx:id="endTableColumn" editable="false" maxWidth="-1.0" minWidth="100.0" prefWidth="-1.0" text="End" />
                <TableColumn fx:id="refTableColumn" editable="false" maxWidth="1.7976931348623157E308" minWidth="120.0" prefWidth="-1.0" text="REF" />
                <TableColumn fx:id="altTableColumn" editable="false" maxWidth="1.7976931348623157E308" minWidth="120.0" prefWidth="-1.0" text="ALT" />
            </columns>
            <placeholder>
                <Label>No variants to display.</Label>
            </placeholder>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
        </TableView>
    </children>
</VBox>
