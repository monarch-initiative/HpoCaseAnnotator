<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.web.WebView?>
<VBox xmlns:fx="http://javafx.com/fxml/1" minHeight="400.0" minWidth="800.0" xmlns="http://javafx.com/javafx/8.0.121"
      fx:controller="org.monarchinitiative.hpo_case_annotator.hpotextmining.controllers.Present">
    <children>
        <SplitPane dividerPositions="0.75" VBox.vgrow="ALWAYS">
            <items>
                <WebView fx:id="webView" minHeight="-Infinity" minWidth="-Infinity" prefWidth="400.0"/>
                <ScrollPane fitToHeight="true" fitToWidth="true" prefWidth="220.0">
                    <content>
                        <SplitPane dividerPositions="0.66" minWidth="195.0" orientation="VERTICAL">
                            <items>
                                <VBox>
                                    <children>
                                        <Label text="HPO terms:">
                                            <font>
                                                <Font name="System Bold" size="16.0"/>
                                            </font>
                                            <VBox.margin>
                                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                            </VBox.margin>
                                        </Label>
                                        <ScrollPane VBox.vgrow="SOMETIMES">
                                            <content>
                                                <VBox fx:id="yesTermsVBox" minHeight="340.0"/>
                                            </content>
                                        </ScrollPane>
                                    </children>
                                </VBox>
                                <VBox>
                                    <children>
                                        <Label text="&quot;NOT&quot; HPO terms:">
                                            <font>
                                                <Font name="System Bold" size="16.0"/>
                                            </font>
                                            <VBox.margin>
                                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                            </VBox.margin>
                                        </Label>
                                        <ScrollPane VBox.vgrow="SOMETIMES">
                                            <content>
                                                <VBox fx:id="notTermsVBox" minHeight="160.0"/>
                                            </content>
                                        </ScrollPane>
                                    </children>
                                </VBox>
                            </items>
                        </SplitPane>
                    </content>
                </ScrollPane>
            </items>
        </SplitPane>
        <HBox VBox.vgrow="NEVER">
            <children>
                <ProgressIndicator fx:id="miningProgressIndicator" progress="0.0" HBox.hgrow="NEVER">
                    <HBox.margin>
                        <Insets top="5.0"/>
                    </HBox.margin>
                </ProgressIndicator>
                <Label fx:id="miningProgressLabel" maxWidth="1.7976931348623157E308" minHeight="30.0"
                       HBox.hgrow="ALWAYS">
                    <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </HBox.margin>
                </Label>
                <Button fx:id="cancelButton" minHeight="30.0" minWidth="120.0" mnemonicParsing="false"
                        onAction="#cancelButtonAction" text="Cancel">
                    <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </HBox.margin>
                </Button>
                <Button fx:id="addTermsButton" alignment="CENTER" minHeight="30.0" minWidth="120.0"
                        mnemonicParsing="false" onAction="#addTermsButtonAction" text="Add selected terms"
                        HBox.hgrow="NEVER">
                    <tooltip>
                        <Tooltip text="Add selected terms into the approved terms table"/>
                    </tooltip>
                    <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </HBox.margin>
                </Button>
            </children>
        </HBox>
    </children>
</VBox>
