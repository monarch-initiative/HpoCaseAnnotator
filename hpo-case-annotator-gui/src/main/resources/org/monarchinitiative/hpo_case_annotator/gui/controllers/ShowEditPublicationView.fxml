<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<StackPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.121" fx:controller="org.monarchinitiative.hpo_case_annotator.gui.controllers.ShowEditPublicationController">
    <children>
        <GridPane>
            <columnConstraints>
                <ColumnConstraints halignment="RIGHT" hgrow="NEVER" />
                <ColumnConstraints halignment="CENTER" hgrow="ALWAYS" minWidth="50.0" prefWidth="600.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="50.0" />
                <RowConstraints />
                <RowConstraints minHeight="30.0" />
                <RowConstraints minHeight="10.0" />
                <RowConstraints minHeight="30.0" />
                <RowConstraints minHeight="30.0" />
                <RowConstraints minHeight="30.0" />
                <RowConstraints minHeight="30.0" />
            </rowConstraints>
            <children>
                <Label text="Publication:" GridPane.columnIndex="1" GridPane.halignment="LEFT">
                    <font>
                        <Font name="System Bold" size="21.0" />
                    </font>
                    <GridPane.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                    </GridPane.margin>
                </Label>
                <Label text="Title:" GridPane.rowIndex="1" />
                <Label text="Authors:" GridPane.rowIndex="2" />
                <Label text="Journal:" GridPane.rowIndex="3" />
                <Label text="Year:" GridPane.rowIndex="4" />
                <Label text="Volume:" GridPane.rowIndex="5" />
                <Label text="Pages:" GridPane.rowIndex="6" />
                <Label text="PMID:" GridPane.rowIndex="7" />
                <TextField fx:id="titleTextField" minHeight="30.0" GridPane.columnIndex="1" GridPane.rowIndex="1" GridPane.vgrow="NEVER">
                    <GridPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </GridPane.margin>
                </TextField>
                <TextField fx:id="authorsTextField" minHeight="30.0" GridPane.columnIndex="1" GridPane.rowIndex="2">
                    <GridPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </GridPane.margin>
                </TextField>
                <TextField fx:id="journalTextField" minHeight="30.0" GridPane.columnIndex="1" GridPane.rowIndex="3">
                    <GridPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </GridPane.margin>
                </TextField>
                <TextField fx:id="yearTextField" minHeight="30.0" GridPane.columnIndex="1" GridPane.rowIndex="4">
                    <GridPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </GridPane.margin>
                </TextField>
                <TextField fx:id="volumeTextField" minHeight="30.0" GridPane.columnIndex="1" GridPane.rowIndex="5">
                    <GridPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </GridPane.margin>
                </TextField>
                <TextField fx:id="pagesTextField" minHeight="30.0" GridPane.columnIndex="1" GridPane.rowIndex="6">
                    <GridPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </GridPane.margin>
                </TextField>
                <TextField fx:id="pmidTextField" minHeight="30.0" GridPane.columnIndex="1" GridPane.rowIndex="7">
                    <GridPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </GridPane.margin>
                </TextField>
            </children>
            <StackPane.margin>
                <Insets bottom="5.0" left="25.0" right="5.0" top="5.0" />
            </StackPane.margin>
        </GridPane>
    </children>
</StackPane>
