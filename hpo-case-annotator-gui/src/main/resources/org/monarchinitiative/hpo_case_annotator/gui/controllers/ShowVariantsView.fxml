<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.StackPane?>
<StackPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/10.0.2-internal"
           fx:controller="org.monarchinitiative.hpo_case_annotator.gui.controllers.ShowVariantsController">
    <children>
        <TableView fx:id="variantsTableView" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                   StackPane.alignment="CENTER">
            <columns>
                <TableColumn fx:id="pmidTableColumn" editable="false" maxWidth="-1.0" minWidth="100.0" prefWidth="100.0"
                             resizable="false" text="PMID"/>
                <TableColumn fx:id="publicationTableColumn" editable="false" maxWidth="-1.0" minWidth="350.0"
                             prefWidth="350.0" text="Publication"/>
                <TableColumn fx:id="variantTableColumn" editable="false" maxWidth="-1.0" minWidth="180.0"
                             prefWidth="180.0" text="Variant"/>
                <TableColumn fx:id="geneTableColumn" editable="false" maxWidth="-1.0" minWidth="100.0" prefWidth="100.0"
                             resizable="false" text="Gene"/>
                <TableColumn fx:id="variantClassTableColumn" editable="false" maxWidth="-1.0" minWidth="120.0"
                             prefWidth="120.0" text="Variant class"/>
                <TableColumn fx:id="pathomechanismTableColumn" editable="false" maxWidth="-1.0" minWidth="200.0"
                             prefWidth="200.0" text="Pathomechanism"/>
                <TableColumn fx:id="consequenceTableColumn" editable="false" maxWidth="-1.0" minWidth="300.0"
                             prefWidth="300.0" text="Consequence"/>
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
        </TableView>
    </children>
</StackPane>
